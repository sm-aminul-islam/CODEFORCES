import java.util.Scanner;

public class wrongSubtraction {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int  a= sc.nextInt();
        int b = sc.nextInt();
        int count=0;
        while (b>=a)
        {
            a*=3;
            b*=2;
            count++;
        }
        System.out.println(count);

    }

}
